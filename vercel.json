{
  "version": 2,
  "build": {
    "env": {
      "NEXT_PUBLIC_BASE_DOMAIN": "nepdora.com",
      "NEXT_PUBLIC_PROTOCOL": "https",
      "NEXT_PUBLIC_ENABLE_SUBDOMAIN_ROUTING": "true",
      "NODE_ENV": "production"
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/account/:path*",
      "destination": "/account/:path*"
    },
    {
      "source": "/login/:path*",
      "destination": "/login/:path*"
    },
    {
      "source": "/signup/:path*",
      "destination": "/signup/:path*"
    },
    {
      "source": "/auth/:path*",
      "destination": "/auth/:path*"
    },
    {
      "source": "/preview/:path*",
      "destination": "/preview/:path*"
    },
    {
      "source": "/builder/:path*",
      "destination": "/builder/:path*"
    }
  ]
}
