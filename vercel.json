{
  "version": 2,
  "build": {
    "env": {
      "NEXT_PUBLIC_BASE_DOMAIN": "nepdora.com",
      "NEXT_PUBLIC_PROTOCOL": "https",
      "NEXT_PUBLIC_ENABLE_SUBDOMAIN_ROUTING": "true"
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}
